{
  "name": "tflori/riki-skeleton",
  "description": "Skeleton application using riki framework",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.1",
    "ext-mbstring": "*",
    "tflori/riki-framework": "dev-master",
    "tflori/dependency-injector": "^2.0",
    "tflori/http": "^1.8",
    "ulrichsg/getopt-php": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Skeleton\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "bin/post-create"
    ]
  }
}
